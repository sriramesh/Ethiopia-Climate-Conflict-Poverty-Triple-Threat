na.color = NA) # Define palette
leaflet() %>% # Plot
addProviderTiles("CartoDB.Positron") %>%
addRasterImage(x = ETH_elev,
color = raster_colorPal_elev) %>%
addLegend(title = "Elevation<br>(meters)",
values = values(ETH_elev),
pal = raster_colorPal_elev)
